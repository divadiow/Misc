CONFIG_L2_PACKET=rwnx
CONFIG_TLS=internal
CONFIG_OS=freeRTOS
CONFIG_MAIN=main_rwnx
CFLAGS +=-D_FREERTOS -DOS_NO_C_LIB_DEFINES
CONFIG_NO_RANDOM_POOL=y
CONFIG_NO_GITVER=y
CONFIG_NO_CONFIG_WRITE=y
CONFIG_NO_CONFIG_BLOBS=y
CONFIG_ELOOP=eloop_freeRTOS
CONFIG_BACKEND=eswin
CONFIG_DRIVER_ESWIN=y
CONFIG_CTRL_IFACE=freeRTOS

CONFIG_IEEE8021X_EAPOL=y

CONFIG_AP=y
NEED_AP_MLME=y

CONFIG_IEEE80211N=y
CONFIG_IEEE80211AX=y
NEED_SME=y
CONFIG_HT_OVERRIDES=y
CONFIG_NO_STDOUT_DEBUG=y
CONFIG_NO_HOSTAPD_LOGGER=y
